<% include includes/header.ejs %>
        <div class="container">
            <% if (message != '') { %>
                <p class="text-center text-danger"><%= message %></p>
            <% } %>
            <form class="add-store-form" action="" method="post" enctype="multipart/form-data">
                <div class="form-row">
                    <div class="form-group col-md-4">
                        <input type="text" class="form-control" name="key" id="key" placeholder="key" required>
                    </div>
                    <div class="form-group col-md-4">
                        <input type="text" class="form-control" name="value" id="number" placeholder="value" required>
                    </div>
                </div>
                <button type="submit" class="btn btn-primary float-right">Add a key-value</button>
            </form>
        </div>
<% include includes/footer.ejs %>